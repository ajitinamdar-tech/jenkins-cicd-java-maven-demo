pipeline {
  agent any
  stages {
    //Use this code for inline pipeline script option
   stage('Code checkout') {
      steps {
        echo "checkout stage"
      }

    }
    stage('Build') {
      steps {
        // Run the maven build
        echo "build stage"
      }

    }
    stage('Deploy') {
      steps {
       echo "deploy stage"

      }
    }
  }
}
